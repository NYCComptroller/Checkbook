{{ attach_library('widget_config/checkbook_project.contracts') }}

<div>
  <h3>
    Contract History
  </h3>
  <table class="outerTable ma-history">
    <thead>
    <tr>
      <th class="text">{{ customWidgetLabel("fiscal_year") | raw }}</th>
      <th class="text">{{ customWidgetLabel("no_of_mod") | raw }}</th>
      <th class="number">{{ customWidgetLabel("current_amount") | raw }}</th>
