var to_position = indexOf(short_filename,&apos;.txt&apos;);

var from_position = to_position - 14;

var file_timestamp = &quot;&quot;;

if(data_source_code != &apos;PC&apos;)
file_timestamp = substr(short_filename,from_position,14);


var patterm_match_sql = &quot;SELECT  (CASE WHEN  &apos;original_file_name&apos; SIMILAR TO &apos;pattern_name&apos; = &apos;t&apos; THEN &apos;Y&apos; ELSE &apos;N&apos; END)::varchar(1) as pattern_matched_flag&quot;;


patterm_match_sql = replace(patterm_match_sql,&quot;original_file_name&quot;,short_filename);

patterm_match_sql = replace(patterm_match_sql,&quot;pattern_name&quot;,actual_pattern);


var file_already_processed_sql = &quot;select count(*) as file_processed_count from etl.etl_data_load_file a , etl.etl_data_load b where  a.load_id = b.load_id and (processed_flag = &apos;Y&apos; OR data_source_code in (&apos;A&apos;,&apos;D&apos;,&apos;E&apos;,&apos;L&apos;,&apos;O&apos;,&apos;RC&apos;,&apos;RY&apos;,&apos;RS&apos;,&apos;BC&apos;,&apos;FC&apos;,&apos;V&apos;)) and file_name = &apos;&quot;+short_filename + &quot;&apos;&quot; + &quot; and b.job_id &lt; &quot; + nextval;

var not_used_sql = &quot;select count(*) as file_processed_count from etl.etl_data_load_file where  processed_flag = &apos;Y&apos; and file_name = &apos;&quot;+short_filename + &quot;&apos;&quot;;

if(data_source_code == &apos;PC&apos;)
file_already_processed_sql = &quot;select count(*) as file_processed_count from etl.etl_data_load_file a JOIN etl.etl_data_load b ON a.load_id = b.load_id where  b.data_source_code != &apos;PC&apos; AND processed_flag = &apos;Y&apos; and file_name = &apos;&quot;+short_filename + &quot;&apos;&quot;;


var file_already_consumed_sql = &quot;select count(*) as file_consumed_count from etl.etl_data_load_file where  load_id =&quot;+load_id +&quot; and file_name = &apos;&quot;+short_filename + &quot;&apos;&quot;;</jsScript_script>
