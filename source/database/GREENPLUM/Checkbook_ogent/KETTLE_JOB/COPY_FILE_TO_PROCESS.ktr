    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>&#47;&#47;Script here

var source_dir_file = source_dir + filename;
var dest_dir_file = dest_dir + filename;
var gpdist_dir_file = gpdist_dir + filename;
var gpdist_std_dir_file = gpdist_dir + standard_file_name;

if (fileExists(source_dir_file)) {
copyFile(source_dir_file,dest_dir_file,true);
copyFile(source_dir_file,gpdist_std_dir_file,true);

}</jsScript_script>
