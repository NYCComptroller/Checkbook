    <sql>SELECT d.data_source_name::varchar(255) as &quot;Data Source Name&quot;,a.agency_code as &quot;Agency Number&quot;, a.fms_contract_number as &quot;Registration Contract ID&quot;, a.fms_commodity_line as &quot;Commodity Line&quot;,
 a.oge_contract_number as &quot;OGE Contract #&quot;, a.purpose as &quot;Contract Purpose&quot;, a.budget_name as &quot;Budget Name&quot;, a.oge_registered_amount as &quot;OGE Registered Amt&quot;, a.vendor_name as &quot;Prime Contractor Name&quot;, 
a.vendor_address as &quot;Prime Contractor Address&quot;, a.vendor_city as &quot;Prime Contractor City&quot;, a.vendor_state as &quot;Prime Contractor State&quot;, a.vendor_zip as &quot;Prime Contractor Zip&quot;  
FROM (SELECT a.agency_code , a.fms_contract_number, a.fms_commodity_line, a.oge_contract_number, a.purpose, a.budget_name, a.oge_registered_amount, a.vendor_name, a.vendor_address, a.vendor_city, 
a.vendor_state, a.vendor_zip, a.created_load_id FROM oge_contract_previous_load a LEFT JOIN oge_contract  b ON a.fms_contract_number = b.fms_contract_number AND a.fms_commodity_line = b.fms_commodity_line 
WHERE b.fms_contract_number IS NULL) a, etl.etl_data_load_file  b, etl.etl_data_load c, etl.ref_data_source d where a.created_load_id =b.load_id AND b.load_id = c.load_id 
AND c.data_source_code =d.data_source_code AND d.table_order = 1 ;
</sql>
