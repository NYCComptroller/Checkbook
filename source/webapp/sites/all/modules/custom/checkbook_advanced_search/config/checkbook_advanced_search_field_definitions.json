{
    "department":{
        "field_type":"select",
        "field_name":"department"
    },
    "expense_category":{
        "field_type":"select",
        "field_name":"expense_category"
    },
    "expense_type":{
        "field_type":"select",
        "field_name":"expense_type",
        "attributes":{
            "title":"Spending Category",
            "options":"$expense_attributes"
        }
    },
    "payee_name":{
        "field_type":"textfield",
        "field_name":"payee_name",
        "attributes":{
            "size":30,
            "maxlength":100
        }
    },
    "check_amount":{
        "field_type":"rangefield",
        "field_name":"check_amount",
        "attributes":{
            "size":30,
            "maxlength":32
        }
    },
    "document_id":{
        "field_type":"textfield",
        "field_name":"document_id",
        "attributes":{
            "size":30,
            "maxlength":100,
            "title":"Document ID"
        }
    },
    "capital_project":{
        "field_type":"textfield",
        "field_name":"capital_project",
        "attributes":{
            "size":30,
            "maxlength":100
        }
    },
    "date_filter":{
        "field_type":"date_filter",
        "field_name":"date_filter"
    },
    "spending":{
        "field_type":"submit",
        "field_name":"spending"
    },
    "domain_filter":{
        "field_type":"radios",
        "field_name":"advanced_search_domain_filter",
        "attributes":{
            "options":{
                "checkbook":"Citywide Agencies",
                "checkbook_oge":"Other Government Entities"
            },
            "default_value":"checkbook",
            "ajax":{
                "event":"change",
                "callback":"checkbook_advanced_search_filter_ajax",
                "method":"replace"
            }
        }
    },
    "status":{
        "field_type":"select",
        "field_name":"status",
        "attributes":{
            "options":{
                "A":"Active",
                "R":"Registered",
                "P":"Pending"
            },
            "default_value":""
        }
    },
    "vendor_name":{
        "field_type":"textfield",
        "field_name":"vendor_name",
        "attributes":{
            "size":30,
            "maxlength":150,
            "title":"Vendor"
        }
    },
    "prime_vendor_name":{
        "field_type":"textfield",
        "field_name":"vendor_name",
        "attributes":{
            "size":30,
            "maxlength":150,
            "title":"Vendor"
        }
    },
    "type":{
        "field_type":"select",
        "field_name":"type",
        "attributes":{
            "options":"$contract_type_attribute",
            "title":"Contract Type"
        }
    },
    "contract_id":{
        "field_type":"textfield",
        "field_name":"contract_num",
        "attributes":{
            "size":30,
            "maxlength":32,
            "title":"Contract ID"
        }
    },
    "pin":{
        "field_type":"textfield",
        "field_name":"pin",
        "attributes":{
            "size":30,
            "maxlength":100,
            "title":"PIN"
        }
    },
    "current_contract_amount":{
        "field_type":"rangefield",
        "field_name":"current_contract_amount",
        "attributes":{
            "size":30,
            "maxlength":32,
            "title":"Current Amount"
        }
    },
    "end_date":{
        "field_type":"range",
        "field_name":"end_date",
        "attributes":{
            "date_year_range":"$year_range"
        }
    },
    "registration_date":{
        "field_type":"range",
        "field_name":"registration_date",
        "attributes":{
            "date_year_range":"$year_range"
        }
    },
    "category":{
        "field_type":"select",
        "field_name":"category",
        "attributes":{
            "options":{
                "all":"All",
                "expense":"Expense",
                "revenue":"Revenue"
            }
        }
    },
    "purpose":{
        "field_type":"textfield",
        "field_name":"purpose",
        "attributes":{
            "size":30,
            "maxlength":100
        }
    },
    "agency":{
        "field_type":"select",
        "field_name":"agency",
        "attributes":{
            "options":"$agencies",
            "option_attributes":"$agency_attributes"
        }
    },
    "apt_pin":{
        "field_type":"textfield",
        "field_name":"apt_pin",
        "attributes":{
            "size":30,
            "maxlength":100,
            "title":"APT PIN"
        }
    },
    "award_method":{
        "field_type":"select",
        "field_name":"award_method",
        "attributes":{
            "options":"$award_method",
            "option_attributes":"$award_method_attributes"
        }
    },
    "start_date":{
        "field_type":"range",
        "field_name":"start_date",
        "attributes":{
            "date_year_range":"$year_range"
        }
    },
    "received_date":{
        "field_type":"range",
        "field_name":"received_date",
        "attributes":{
            "date_year_range":"$year_range"
        }
    },
    "year":{
        "field_type":"select",
        "field_name":"year",
        "attributes":{
            "options":"$year_attribute"
        }
    },
    "contracts":{
        "field_type":"submit",
        "field_name":"contracts"
    },
    "entity_contract_number":{
        "field_type":"textfield",
        "field_name":"entity_contract_number",
        "attributes":{
            "size":30,
            "maxlength":32,
            "title":"Entity Contract #"
        }
    },
    "commodity_line":{
        "field_type":"textfield",
        "field_name":"commodity_line",
        "attributes":{
            "size":30,
            "maxlength":32
        }
    },
    "budget_name":{
        "field_type":"textfield",
        "field_name":"budget_name",
        "attributes":{
            "size":30,
            "maxlength":100
        }
    },
    "other_government_entities":{
        "field_type":"select",
        "field_name":"agency",
        "attributes":{
            "title":"Other Government<br/> Entity",
            "options":"$agencies",
            "option_attributes":"$agency_attributes"
        }
    },
    "mwbe_category":{
        "field_type":"select",
        "field_name":"mwbe_category",
        "attributes":{
            "title":"M/WBE Category",
            "options":"$mwbe_category_attributes"
        }
    },
    "industry":{
        "field_type":"select",
        "field_name":"industry",
        "attributes":{
            "title":"Industry",
            "options":"$industry_attributes"
        }
    },
    "includes_sub_vendors":{
        "field_type":"select",
        "field_name":"includes_sub_vendors",
        "attributes":{
            "title":"Contract Includes<br/>Sub Vendors",
            "options": "$includes_sub_vendors",
            "option_attributes": "$includes_sub_vendors_attributes"
        }
    },
    "sub_vendor_status":{
        "field_type":"select",
        "field_name":"sub_vendor_status",
        "attributes":{
            "title":"Sub Vendor<br/>Status in PIP",
            "options":"$sub_vendor_status",
            "option_attributes":"$sub_vendor_status_attributes"
        }
    },
    "purchase_order_type":{
      "field_type":"select",
      "field_name":"purchase_order_type",
      "attributes":{
        "title":"Purchase Order<br/>Type",
        "options":{
          "All":"All",
          "BA":"Blanket Agreement",
          "PA":" Planned Agreement",
          "PO":"Purchase Order"
        },
        "default_value":"All"
      }
    },
    "purchase_order_number":{
      "field_type":"textfield",
      "field_name":"purchase_order_number",
      "attributes":{
        "title":"Contract ID"
      }
    },
    "responsibility_center":{
      "field_type":"select",
      "field_name":"responsibility_center",
      "attributes":{
        "title":"Responsibility<br/>Center",
        "options":"$nycha_responsibility_center_attributes"
      }
    },
    "nycha_contract_type":{
      "field_type":"select",
      "field_name":"nycha_contract_type",
      "attributes":{
        "options":"$nycha_contract_type_attributes",
        "title":"Contract Type"
      }
    },
   "nycha_award_method":{
      "field_type":"select",
      "field_name":"nycha_award_method",
      "attributes":{
        "title": "Award Method",
        "options":"$nycha_award_method_attributes"
      }
   },
   "nycha_industry":{
      "field_type":"select",
      "field_name":"nycha_industry",
      "attributes":{
        "title":"Industry",
        "options":"$nycha_industry_attributes"
      }
   },
    "approved_date":{
      "field_type":"range",
      "field_name":"approved_date",
      "attributes":{
        "date_year_range":"$year_range"
      }
    }
}
