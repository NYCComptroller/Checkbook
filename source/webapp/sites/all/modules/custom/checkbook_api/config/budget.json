                        "adopted":"adopted_amount",
                        "encumbered":"encumbered_amount",
                        "cash_expense":"cash_expense_amount"
                    },
                    "rowElements":[
                      "agency",
                      "year",
                      "department",
                      "expense_category",
                      "budget_code",
                      "budget_name"
                    ],
                    "rowDataGroupElements":{
                        "budget_amounts":[
                          "modified",
                          "adopted"
                        ],
                        "expenditure_amounts":[
                          "pre_encumbered",
                          "encumbered",
                          "cash_expense",
                          "post_adjustment",
                          "accrued_expense"
                        ]
                    }
                },
                "csv":{
                    "elementsColumn":{
                        "Agency":"agency_name",
                        "Department":"department_name",
                        "Expense Category":"object_class_name",
                        "Budget Code":"budget_code",
                        "Budget Name":"budget_code_name",
                        "Pre-Encumbered":"pre_encumbered_amount",
                        "Accrued Expense":"accrued_expense_amount",
                        "Year":"budget_fiscal_year",
                        "Post Adjustments":"post_closing_adjustment_amount",
                        "Modified":"current_budget_amount",
                        "Adopted":"adopted_amount",
                        "Encumbered":"encumbered_amount",
                        "Cash Expense":"cash_expense_amount"
                    }
                }
            },
            "adjustParameters":"if(function_exists('checkbook_api_adjustBudgetParameterFilters')){checkbook_api_adjustBudgetParameterFilters($data_set,$parameters,$criteria);}"
        },
        "requestParameters":{
            "year":{
                "valueType":"value",
                "dataType":"year",
                "maxLength":4
            },
            "budget_code":{
                "valueType":"value",
                "dataType":"text",
