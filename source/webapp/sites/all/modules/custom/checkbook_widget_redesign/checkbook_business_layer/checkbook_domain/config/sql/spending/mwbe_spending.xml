            SELECT a.agency_name,
                   v.legal_name AS prime_vendor_name,
                   s0.type_of_year,
                   s0.year_id,
                   s0.agency_id,
                   s0.vendor_id as prime_vendor_id,
                   s0.description,
                   s0.document_id,
                   s0.document_code,
                   s0.agreement_id, 
                   SUM(COALESCE(total_spending_amount,0)) AS check_amount_sum,
