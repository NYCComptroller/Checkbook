{
    "datasets": {
        "subcontract_status_by_prime_contract_id": {
            "datasourceName": "main",
            "source": "subcontract_status_by_prime_contract_id"
        },

        "mixed_vendor_data":{
            "datasourceName":"main",
            "source":"SELECT prime_vendor_code as vendor_code, prime_vendor_name as vendor_name, * FROM subcontract_status_by_prime_contract_id WHERE prime_sub_vendor_code_by_type ~* '(^P:.*$)|(.*,P:.*$)|(^P:.*,.*)|(^PM:.*$)|(.*,PM:.*$)|(^PM:.*,.*)'
                      UNION ALL
                      SELECT sub_vendor_code as vendor_code, sub_vendor_name as vendor_name, * FROM subcontract_status_by_prime_contract_id WHERE prime_sub_vendor_code_by_type ~* '(^S:.*$)|(.*,S:.*$)|(^S:.*,.*)|(^SM:.*$)|(.*,SM:.*$)|(^SM:.*,.*)'
                      "
        },
        "mixed_vendor_type_data":{
            "datasourceName":"main",
            "source":"SELECT 'PRIME VENDOR' vendor_type, 'P' as vendor_type_id, prime_vendor_code as vendor_code, * FROM subcontract_status_by_prime_contract_id WHERE prime_sub_vendor_code_by_type ~* '(^P:.*$)|(.*,P:.*$)|(^P:.*,.*)|(^PM:.*$)|(.*,PM:.*$)|(^PM:.*,.*)'
                      UNION ALL
                      SELECT 'SUB VENDOR' as vendor_type, 'S' as vendor_type_id, sub_vendor_code as vendor_code, * FROM subcontract_status_by_prime_contract_id WHERE prime_sub_vendor_code_by_type ~* '(^S:.*$)|(.*,S:.*$)|(^S:.*,.*)|(^SM:.*$)|(.*,SM:.*$)|(^SM:.*,.*)'
                      UNION ALL
                      SELECT 'M/WBE VENDOR' vendor_type, 'M' as vendor_type_id, prime_vendor_code as vendor_code, * FROM subcontract_status_by_prime_contract_id WHERE prime_sub_vendor_code_by_type ~* '(^PM:.*$)|(.*,PM:.*$)|(^PM:.*,.*)'
                      UNION ALL
                      SELECT 'M/WBE VENDOR' as vendor_type, 'M' as vendor_type_id, sub_vendor_code as vendor_code, * FROM subcontract_status_by_prime_contract_id WHERE prime_sub_vendor_code_by_type ~* '(^SM:.*$)|(.*,SM:.*$)|(^SM:.*,.*)'
                      "
        },
        "mixed_minority_type_data":{
            "datasourceName":"main",
            "source":"SELECT CASE WHEN prime_minority_type_id IN (4,5) THEN '4~5' ELSE CAST(prime_minority_type_id as text) END AS minority_type_id, prime_minority_type_name as minority_type_name, prime_vendor_code as vendor_code, * FROM subcontract_status_by_prime_contract_id
                      UNION ALL
                      SELECT CASE WHEN sub_minority_type_id IN (4,5) THEN '4~5' ELSE CAST(sub_minority_type_id as text) END AS minority_type_id, sub_minority_type_name as minority_type_name, sub_vendor_code as vendor_code, * FROM subcontract_status_by_prime_contract_id
            "
        }
    },
