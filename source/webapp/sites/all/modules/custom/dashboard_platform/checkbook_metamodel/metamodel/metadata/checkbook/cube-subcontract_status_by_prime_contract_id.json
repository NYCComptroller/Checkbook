{
    "datasets": {
        "subcontract_status_by_prime_contract_id": {
            "datasourceName": "main",
            "source": "subcontract_status_by_prime_contract_id"
        },
        "mixed_vendor_data":{
            "datasourceName":"main",
            "source":"SELECT prime_vendor_code as vendor_code, prime_vendor_name as vendor_name,
            CASE WHEN prime_minority_type_id IN (2,3,4,5,9) THEN 'PM' ELSE 'P' END as vendor_type,
            prime_minority_type_id as minority_type_id, prime_minority_type_name as minority_type_name, *
            FROM subcontract_status_by_prime_contract_id
            WHERE prime_sub_vendor_code_by_type ~* '(^P:.*$)|(.*,P:.*$)|(^P:.*,.*)|(^PM:.*$)|(.*,PM:.*$)|(^PM:.*,.*)'

            UNION ALL

            SELECT sub_vendor_code as vendor_code, sub_vendor_name as vendor_name,
            CASE WHEN sub_minority_type_id IN (2,3,4,5,9) THEN 'SM' ELSE 'S' END as vendor_type,
            sub_minority_type_id as minority_type_id, sub_minority_type_name as minority_type_name, *
            FROM subcontract_status_by_prime_contract_id
            WHERE prime_sub_vendor_code_by_type ~* '(^S:.*$)|(.*,S:.*$)|(^S:.*,.*)(^SM:.*$)|(.*,SM:.*$)|(^SM:.*,.*)'
            "
        },
        "mixed_vendor_type_data":{
            "datasourceName":"main",
            "source":"SELECT 'PRIME VENDOR' vendor_type_name, 'P' as vendor_type_id, prime_vendor_code as vendor_code, prime_minority_type_id as minority_type_id, CASE WHEN prime_minority_type_id IN (2,3,4,5,9) THEN 'PM' ELSE 'P' END as vendor_type, * FROM subcontract_status_by_prime_contract_id
                    WHERE prime_sub_vendor_code_by_type ~* '(^P:.*$)|(.*,P:.*$)|(^P:.*,.*)|(^PM:.*$)|(.*,PM:.*$)|(^PM:.*,.*)'
                    UNION ALL
                    SELECT 'SUB VENDOR' as vendor_type_name, 'S' as vendor_type_id, sub_vendor_code as vendor_code, sub_minority_type_id as minority_type_id, CASE WHEN sub_minority_type_id IN (2,3,4,5,9) THEN 'SM' ELSE 'S' END as vendor_type, * FROM subcontract_status_by_prime_contract_id
                    WHERE prime_sub_vendor_code_by_type ~* '(^S:.*$)|(.*,S:.*$)|(^S:.*,.*)|(^SM:.*$)|(.*,SM:.*$)|(^SM:.*,.*)'
                    UNION ALL
                    SELECT 'M/WBE VENDOR' vendor_type_name, 'PM' as vendor_type_id, prime_vendor_code as vendor_code, prime_minority_type_id as minority_type_id, CASE WHEN prime_minority_type_id IN (2,3,4,5,9) THEN 'PM' ELSE 'P' END as vendor_type, * FROM subcontract_status_by_prime_contract_id
                    WHERE prime_sub_vendor_code_by_type ~* '(^PM:.*$)|(.*,PM:.*$)|(^PM:.*,.*)'
                    UNION ALL
                    SELECT 'M/WBE VENDOR' as vendor_type_name, 'SM' as vendor_type_id, sub_vendor_code as vendor_code, sub_minority_type_id as minority_type_id, CASE WHEN sub_minority_type_id IN (2,3,4,5,9) THEN 'SM' ELSE 'S' END as vendor_type, * FROM subcontract_status_by_prime_contract_id
                    WHERE prime_sub_vendor_code_by_type ~* '(^SM:.*$)|(.*,SM:.*$)|(^SM:.*,.*)'
                    "
        }
    },
