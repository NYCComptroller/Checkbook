{
  "datasets":{
    "budget_percent_difference":{
      "datasourceName":"main",
      "source":"aggregateon_budget_by_year"
    }
  },
  "cubes":{
    "budget_percent_difference":{
      "sourceDatasetName":"budget_percent_difference",
      "dimensions":[
        {
          "name": "year_id",
          "levels": [
            {
              "name": "year_id",
              "sourceColumnName": "budget_fiscal_year_id"
            }
          ]
        },
        {
          "name": "expenditure_type_name",
          "levels": [
            {
              "name": "expenditure_type_name",
              "sourceColumnName": "expenditure_type_name"
            }
          ]
        },
        {
          "name": "exp_budget",
          "levels": [
            {
              "name": "exp_budget",
              "datasetName":"exp_budget",
              "sourceColumnName": "expenditure_type_name"
            }
          ]
        },
        {
          "name":"funding_source_code",
          "levels":[
            {
              "name":"funding_source_code",
              "sourceColumnName":"funding_source_code"
            }
          ]
        },
        {
          "name":"funding_budget",
          "levels":[
            {
              "name":"funding_budget",
              "datasetName":"funding_budget",
              "sourceColumnName":"funding_source_code"
            }
          ]
        },
        {
          "name":"responsibility_center_code",
          "levels":[
            {
              "name":"responsibility_center_code",
              "sourceColumnName":"responsibility_center_code"
            }
          ]
        },
        {
          "name":"resp_budget",
          "levels":[
            {
