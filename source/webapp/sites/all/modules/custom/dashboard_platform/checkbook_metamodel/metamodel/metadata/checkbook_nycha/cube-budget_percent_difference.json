            }
          ]
        },
        {
          "name":"prog_budget",
          "levels":[
            {
              "name":"prog_budget",
              "datasetName":"prog_budget",
              "sourceColumnName":"program_phase_code"
            }
          ]
        },
        {
          "name":"gl_project_code",
          "levels":[
            {
              "name":"gl_project_code",
              "sourceColumnName":"gl_project_code"
            }
          ]
        },
        {
          "name":"proj_budget",
          "levels":[
            {
              "name":"proj_budget",
