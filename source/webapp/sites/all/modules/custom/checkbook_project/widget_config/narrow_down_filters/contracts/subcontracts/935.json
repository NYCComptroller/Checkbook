    "adjustParameters":"

        if(isset($parameters['minority_type_id'])) {
            $minority_type_id = $parameters['minority_type_id'];
        }
        $parameters = ContractUtil::adjustContractParameterFilters($node, $parameters);
        if(function_exists('_checkbook_project_applyParameterFilters')){
            $parameters = _checkbook_project_applyParameterFilters($node,$parameters);
            $parameters['vendor_type.vendor_type'][] = data_controller_get_operator_factory_instance()->initiateHandler(NotEmptyOperatorHandler::$OPERATOR__NAME);
        }
        if(isset($minority_type_id)) {
            $parameters['minority_type_id'] = $minority_type_id;
        }
        if(isset($parameters['prime_sub_vendor_code'])) {
            $parameters['vendor_code'] = $parameters['prime_sub_vendor_code'];
        }
        unset($parameters['prime_sub_vendor_code_by_type']);
        unset($parameters['prime_sub_vendor_code']);
        unset($parameters['prime_sub_minority_type_id']);

        return $parameters;
    ",
    "template":"individual_filter"
}