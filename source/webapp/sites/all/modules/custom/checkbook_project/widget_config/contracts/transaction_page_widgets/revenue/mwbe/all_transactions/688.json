agencyIds = document.getElementById('fHideAgencyId').value;
}
var oVendorIds = getNamedFilterCriteria('fvendorId');
var oVendorNms = getNamedFilterCriteria('fvendorNm');
var oCurAmount = getNamedFilterCriteria('fCurAmount');
