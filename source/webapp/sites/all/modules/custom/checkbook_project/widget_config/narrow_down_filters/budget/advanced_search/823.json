    "allowFacetDeselect":true,
    "columns":["agency.agency", "agency_name.agency_name", "txcount"],
    "orderBy":"-txcount",    
    "facetNoPager":true,
    "filterName":"Agency",
    "urlParameterName":"agency",
    "autocompleteParamName":"agency_auto",    
    "autocompleteID":"fagencyId",
